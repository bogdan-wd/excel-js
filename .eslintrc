{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "quotes": "off",
        "object-curly-spacing": "off",
        "func-call-spacing": "off",
        "no-trailing-spaces": "off",
        "eol-last": "off",
        "space-before-function-paren": "off"
    },
    "extends": [
        "eslint:recommended",
        "google",
    ],
}